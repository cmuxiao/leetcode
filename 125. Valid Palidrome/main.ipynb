    "class Solution(object):\n",
    "    def isPalindrome(self, s):\n",
    "        \"\"\"\n",
    "        :type s: str\n",
    "        :rtype: bool\n",
    "        \"\"\"\n",
    "        filtered_s = ''.join(char.lower() for char in s if char.isalnum())\n",
    "        return filtered_s == filtered_s[::-1]"
   ]
  }
